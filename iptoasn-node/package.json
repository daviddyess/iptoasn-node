{
  "name": "iptoasn-node",
  "version": "0.9.0",
  "description": "High-performance IP to ASN lookup library with automatic updates (Rust-powered)",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "*.node",
    "README.md",
    "LICENSE"
  ],
  "napi": {
    "binaryName": "iptoasn-node",
    "targets": [
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "aarch64-unknown-linux-gnu"
    ]
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "test": "node ../examples/basic-usage.js",
    "version": "napi version"
  },
  "keywords": [
    "ip",
    "asn",
    "autonomous-system",
    "geolocation",
    "network",
    "rust",
    "napi",
    "native",
    "performance",
    "ip-lookup",
    "asn-lookup",
    "iptoasn",
    "bgp"
  ],
  "author": "David Dyess II",
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0"
  },
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daviddyess/iptoasn-node.git"
  },
  "bugs": {
    "url": "https://github.com/daviddyess/iptoasn-node/issues"
  },
  "homepage": "https://github.com/daviddyess/iptoasn-node#readme",
  "publishConfig": {
    "access": "public"
  },
  "optionalDependencies": {
    "iptoasn-node-linux-x64-gnu": "0.9.0",
    "iptoasn-node-darwin-x64": "0.9.0",
    "iptoasn-node-darwin-arm64": "0.9.0",
    "iptoasn-node-win32-x64-msvc": "0.9.0"
  }
}